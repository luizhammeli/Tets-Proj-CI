default_platform(:ios)

lane :swiftlint do
  swiftlint(
    mode: :lint,        # ou :fix
    strict: true        # falha o build se houver warning/error
  )
end

desc "Run Scheme Tets-Proj-CI"
lane :ci_iOS do
  bund_app(
      project: "./Tets-Proj-CI.xcodeproj"
      scheme: "Tets-Proj-CI",
      export_method: "development",
      skip_codesigning: true,
      skip_archive: true,
      xcarchive: "CODE_SIGNING=NO CODE_SIGNING_REQUIRED=NO"
  )
end